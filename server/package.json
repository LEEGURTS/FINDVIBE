{
  "name": "find_vive",
  "version": "1.0.0",
  "scripts": {
    "client": "cd .. && cd client && yarn start",
    "server": "pm2 start main.js",
    "stop": "pm2 stop main",
    "delete": "pm2 delete main",
    "server_test": "nodemon main.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "build": "cd .. && cd client && npm run build && cd .. && cd server"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "concurrently": "^7.6.0",
    "connect-mongo": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose-session": "^0.0.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.4",
    "nodemon": "^2.0.21",
    "pm2": "^5.3.0",
    "sequelize": "^6.31.0",
    "sequelize-cli": "^6.6.0",
    "yarn": "^1.22.19"
  }
